<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net" />
  </configSections>
  <log4net>
    <appender name="SizeRollingLogFileAppender" type="log4net.Appender.RollingFileAppender">
      <param name="File" value="../logs/adAndonCustomizeTool.log" />
      <param name="AppendToFile" value="true" />
      <param name="RollingStyle" value="size" />
      <param name="MaximumFileSize" value="5MB" />
      <param name="MaxSizeRollBackups" value="9" />
      <param name="StaticLogFileName" value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <param name="ConversionPattern" value="%d{yyyy/MM/dd HH:mm:ss.fff} [%t] %-5p %m%n" />
      </layout>
      <Encoding value="UTF-8" />
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock" />
    </appender>
    <root>
      <level value="DEBUG" />
      <appender-ref ref="SizeRollingLogFileAppender" />
    </root>
  </log4net>
  <System>
    <items>
      <!-- 単体デバッグモードで起動するか(True:する False:しない) -->
      <add key="Debug" value="True" />
      <!-- カテゴリー名 -->
      <add key="Category" value="PDCategory" />
      <!-- データベース 接続タイムアウト(ms) -->
      <add key="DBConnectTimeout" value="10000" />
      <!-- データベース 接続リトライ回数 -->
      <add key="DBRetryCount" value="3" />
      <!-- Smart Operation 接続タイムアウト(ms) -->
      <add key="SmartOperationConnectTimeout" value="10000" />
      <!-- Smart Operation 接続リトライ回数 -->
      <add key="SmartOperationRetryCount" value="3" />
      <!-- 検索条件保存先(パッケージ) -->
      <add key="SearchConditionPackagePath" value="../SearchCondition/Package" />
      <!-- 検索条件保存先(AVL) -->
      <add key="SearchConditionAVLPath" value="../SearchCondition/AVL" />
      <!-- パートメインウィンドウのサイズ、位置 -->
      <add key="MainWndPos" value="﻿&lt;?xml version=&quot;1.0&quot; encoding=&quot;utf-8&quot;?&gt;&lt;WINDOWPLACEMENT xmlns:xsd=&quot;http://www.w3.org/2001/XMLSchema&quot; xmlns:xsi=&quot;http://www.w3.org/2001/XMLSchema-instance&quot;&gt;&lt;length&gt;44&lt;/length&gt;&lt;flags&gt;2&lt;/flags&gt;&lt;showCmd&gt;3&lt;/showCmd&gt;&lt;minPosition&gt;&lt;X&gt;-1&lt;/X&gt;&lt;Y&gt;-1&lt;/Y&gt;&lt;/minPosition&gt;&lt;maxPosition&gt;&lt;X&gt;-1&lt;/X&gt;&lt;Y&gt;-1&lt;/Y&gt;&lt;/maxPosition&gt;&lt;normalPosition&gt;&lt;Left&gt;140&lt;/Left&gt;&lt;Top&gt;272&lt;/Top&gt;&lt;Right&gt;1749&lt;/Right&gt;&lt;Bottom&gt;872&lt;/Bottom&gt;&lt;/normalPosition&gt;&lt;/WINDOWPLACEMENT&gt;" />
      <!-- 現在のパート種 -->
      <add key="CurrentDataType" value="4" />
      <!-- トレイ情報ファイル保存先パス -->
      <add key="TraySettingPath" value="../Ini/TraySetting" />
    </items>
  </System>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>