<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.image.*?>
<?import javafx.geometry.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane style="-fx-background-color: black;" styleClass="ContentSplitPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="adtekfuji.andon.agenda.component.AgendaCompoController">
    <children>
        <AnchorPane fx:id="scalePane" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0">
            <children>
                <AnchorPane fx:id="contentPane" prefHeight="880.0" prefWidth="1780.0" style="-fx-background-color: black;" styleClass="ContentPane" AnchorPane.leftAnchor="100.0" AnchorPane.topAnchor="100.0" />
                <!-- 縦軸 -->
                <AnchorPane fx:id="verticalPane" minHeight="0.0" minWidth="0.0" prefHeight="880.0" prefWidth="100.0" style="-fx-background-color: black;" styleClass="SideNaviPane" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="100.0" />
                <!-- 現在時ライン -->
                <Label fx:id="timeLine" minHeight="-Infinity" prefHeight="2.0" style="-fx-background-color: red;" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
                <!-- 横軸 -->
                <AnchorPane fx:id="horizonPane" prefHeight="100.0" prefWidth="1780.0" style="-fx-background-color: black;" styleClass="SideNaviPane" AnchorPane.leftAnchor="100.0" AnchorPane.topAnchor="0.0" />
                <!-- 目隠し -->
                <AnchorPane fx:id="blindPane" prefHeight="100.0" prefWidth="100.0" style="-fx-background-color: black;" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="00.0" />
            </children>
        </AnchorPane>

        <!-- 縦スクロールバー -->
        <ScrollBar fx:id="scrollBarHeight" orientation="VERTICAL" prefWidth="20.0" AnchorPane.bottomAnchor="20.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
        <!-- 横スクロールバー -->
        <ScrollBar fx:id="scrollBarWidth" orientation="HORIZONTAL" prefHeight="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="20.0" />
        <!-- ズームスライダ -->
        <Slider fx:id="sliderZoom" orientation="VERTICAL" prefHeight="500.0" prefWidth="30.0" AnchorPane.bottomAnchor="20.0" AnchorPane.rightAnchor="20.0" />
        <!-- ズームボタン -->
        <Button fx:id="labelZoom" prefHeight="20.0" prefWidth="20.0" style="-fx-background-image: url('image/btnZoomOFF.PNG'); -fx-background-repeat: no-repeat; -fx-background-position: center center; -fx-border-width: 0;-fx-border-color: black; -fx-padding: 0; -fx-spacing: 0;" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0"/>
    </children>
</AnchorPane>
