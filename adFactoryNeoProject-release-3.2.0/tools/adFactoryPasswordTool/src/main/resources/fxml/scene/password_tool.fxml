<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="100" prefWidth="320" xmlns:fx="http://javafx.com/fxml" fx:controller="jp.adtekfuji.adfactorypasswordtool.scene.PasswordToolFXController">
    <children>
        <GridPane hgap="16.0" vgap="8.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="-Infinity" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
                <RowConstraints minHeight="-Infinity" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Label text="パスワード" />
                <PasswordField fx:id="passwordText" text="" GridPane.columnIndex="1" />
                <Label text="確認用パスワード" GridPane.rowIndex="1" />
                <PasswordField fx:id="passwordChkText" text="" GridPane.rowIndex="1" GridPane.columnIndex="1" />
                <Button text="暗号化" onAction="#encButtonAction" GridPane.rowIndex="2" GridPane.columnIndex="1" />
                <TextField fx:id="encText" text="" GridPane.rowIndex="3" GridPane.columnIndex="1" />
            </children>
            <padding>
                <Insets left="16.0" right="24.0" top="16.0" bottom="16.0" />
            </padding>
        </GridPane>
    </children>
</AnchorPane>
