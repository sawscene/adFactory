<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="adtekfuji.admanagerapp.summaryreportplugin.component.SummaryReportCompoFxController">
    <StackPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <SplitPane fx:id="summaryReportPane" dividerPositions="0.45" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <GridPane SplitPane.resizableWithParent="false">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints vgrow="SOMETIMES"/>
                    <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="50.0" vgrow="SOMETIMES"/>
                </rowConstraints>
                <TreeView fx:id="hierarchyTree">
                    <GridPane.margin>
                        <Insets bottom="8.0"/>
                    </GridPane.margin>
                </TreeView>
                <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1">
                    <ToolBar fx:id="hierarchyBtnArea" nodeOrientation="LEFT_TO_RIGHT" styleClass="TransparentToolbar" prefHeight="40.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                        <Button fx:id="createButton" mnemonicParsing="false" onAction="#onCreate" styleClass="NarrowWidthButton" text="%key.NewCreate"/>
                        <Button fx:id="copyButton" mnemonicParsing="false" onAction="#onCopy" styleClass="NarrowWidthButton" text="%key.Copy"/>
                        <Button fx:id="delButton" mnemonicParsing="false" onAction="#onDelete" styleClass="NarrowWidthButton" text="%key.Delete"/>
                    </ToolBar>
                </AnchorPane>
            </GridPane>
            <GridPane>
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints vgrow="SOMETIMES"/>
                    <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="50.0" vgrow="SOMETIMES"/>
                </rowConstraints>
                <ScrollPane styleClass="ContentSplitPane">
                    <GridPane.margin>
                        <Insets bottom="8.0"/>
                    </GridPane.margin>
                    <VBox>
                        <VBox fx:id="detailPane">
                            <padding>
                                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                            </padding>
                        </VBox>
                        <VBox fx:id="settingPane">
                            <padding>
                                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
                            </padding>
                        </VBox>
                    </VBox>
                </ScrollPane>
                <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1">
                    <HBox alignment="BASELINE_RIGHT" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                        <ToolBar nodeOrientation="LEFT_TO_RIGHT" styleClass="TransparentToolbar">
                            <Button fx:id="sendTestMailButton" mnemonicParsing="false" onAction="#onSendTestMail" styleClass="NarrowWidthButton" text="%key.SendTestMail"/>
                            <Button fx:id="registButton" mnemonicParsing="false" onAction="#onRegist" styleClass="NarrowWidthButton" text="%key.Regist"/>
                        </ToolBar>
                    </HBox>
                </AnchorPane>
            </GridPane>
        </SplitPane>
        <Pane fx:id="Progress" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity">
            <ProgressIndicator maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"/>
        </Pane>
    </StackPane>
</AnchorPane>
