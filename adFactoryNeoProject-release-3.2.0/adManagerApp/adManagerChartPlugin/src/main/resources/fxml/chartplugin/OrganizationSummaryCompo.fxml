<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import jp.adtekfuji.javafxcommon.controls.*?>

<AnchorPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="adtekfuji.admanagerapp.chartplugin.controller.OrganizationSummaryCompoController">
    <children>
        <SplitPane fx:id="organizationSummaryPane" dividerPositions="0.2" orientation="HORIZONTAL" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <items>
                <AnchorPane minWidth="320.0" styleClass="ContentPane" SplitPane.resizableWithParent="false">
                    <children>
                        <!-- 対象日 -->
                        <fx:include fx:id="datePane" source="DatePane.fxml" AnchorPane.topAnchor="8.0" />
                        <!-- 対象工程順 -->
                        <fx:include fx:id="workflowPane" source="WorkflowPane.fxml" AnchorPane.topAnchor="180.0" />
                        <!-- 対象工程 -->
                        <fx:include fx:id="workPane" source="WorkPane.fxml" AnchorPane.topAnchor="480.0" />
                    </children>
                </AnchorPane>
                <AnchorPane styleClass="ContentPane">
                    <children>
                        <AnchorPane prefHeight="140.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <children>
                                <HBox layoutX="8.0" layoutY="29.0" spacing="8.0" AnchorPane.bottomAnchor="0.0">
                                    <children>
                                        <!-- 更新ボタン -->
                                        <Button fx:id="updateButton" mnemonicParsing="false" onAction="#onUpdate" prefHeight="23.0" prefWidth="80.0" text="%key.Update" />
                                        <!-- CSV出力ボタン -->
                                        <Button fx:id="exportButton" mnemonicParsing="false" onAction="#onExport" prefHeight="23.0" prefWidth="80.0" text="%key.OutputAnalysisReport" />
                                        <!-- 画像ファイル出力ボタン -->
                                        <Button fx:id="outputImageFileButton" minWidth="80.0" mnemonicParsing="false" onAction="#onOutputImageFile" prefHeight="23.0" text="%key.OutputImageFile" />
                                    </children>
                                </HBox>
                                <PropertySaveTableView fx:id="summaryView" prefWidth="300.0" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                    <columns>
                                        <TableColumn fx:id="summaryNameColumn" prefWidth="160.0" style="-fx-alignment: CENTER-LEFT;" styleClass="rightAlignedTableColumn" text="%key.PropertyName" />
                                        <TableColumn fx:id="summaryValueColumn" prefWidth="120.0" style="-fx-alignment: CENTER-RIGHT;" styleClass="rightAlignedTableColumn" text="%key.PropertyContent" />
                                    </columns>
                                </PropertySaveTableView>
                                <VBox fx:id="progressPane" alignment="CENTER" layoutX="-374.0" layoutY="-338.0" AnchorPane.bottomAnchor="338.0" AnchorPane.leftAnchor="-374.0" AnchorPane.rightAnchor="374.0" AnchorPane.topAnchor="-338.0">
                                    <children>
                                        <ProgressIndicator fx:id="progressIndicator" prefHeight="80.0" prefWidth="80.0" />
                                    </children>
                                </VBox>
                            </children>
                        </AnchorPane>
                        <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="8.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="144.0">
                            <right>
                            </right>
                            <center>
                                <!-- タイムラインチャート -->
                                <ScrollPane fitToWidth="true">
                                    <content>
                                        <FlowPane fx:id="chartPane" columnHalignment="LEFT" orientation="HORIZONTAL">
                                        </FlowPane>
                                    </content>
                                </ScrollPane>
                            </center>
                        </BorderPane>
                    </children>
                    <padding>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                    </padding>
                </AnchorPane>
            </items>
        </SplitPane>
    </children>
</AnchorPane>
