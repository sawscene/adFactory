<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import jp.adtekfuji.javafxcommon.controls.*?>

<AnchorPane fx:id="MainScenePane" prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="adtekfuji.admanagerapp.systemsettingplugin.component.WorkCategoryEditCompoFxController">
    <children>
        <StackPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <SplitPane fx:id="workCategoryPane" dividerPositions="0.45" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <items>
                        <GridPane SplitPane.resizableWithParent="false">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints vgrow="SOMETIMES" />
                                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="60.0" vgrow="SOMETIMES" />
                            </rowConstraints>
                            <children>
                                <TreeView fx:id="workCategoryTree" onKeyPressed="#onKeyPressed" GridPane.rowIndex="0" />
                                <ToolBar nodeOrientation="LEFT_TO_RIGHT" styleClass="TransparentToolbar" GridPane.rowIndex="1">
                                    <items>
                                        <Button fx:id="createTreeButton" mnemonicParsing="false" onAction="#onTreeCreate" styleClass="NarrowWidthButton" text="%key.NewCreate" />
                                        <Button fx:id="editTreeButton" mnemonicParsing="false" onAction="#onTreeEdit" styleClass="NarrowWidthButton" text="%key.Edit" />
                                        <Button fx:id="deleteTreeButton" mnemonicParsing="false" onAction="#onTreeDelete" styleClass="NarrowWidthButton" text="%key.Delete" />
                                    </items>
                                </ToolBar>
                            </children>
                        </GridPane>
                        <GridPane fx:id="workPane" >
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
                                <RowConstraints vgrow="SOMETIMES" />
                                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="60.0" vgrow="SOMETIMES" />
                            </rowConstraints>
                            <children>
                                <GridPane>
                                    <columnConstraints>
                                        <ColumnConstraints hgrow="SOMETIMES" />
                                    </columnConstraints>
                                    <rowConstraints>
                                        <RowConstraints vgrow="SOMETIMES" />
                                    </rowConstraints>
                                    <children>
                                        <ToolBar nodeOrientation="RIGHT_TO_LEFT">
                                            <items>
                                                <Button fx:id="moveWorkButton" mnemonicParsing="false" onAction="#onMoveWork" styleClass="NarrowWidthButton" text="%key.Move" />
                                            </items>
                                        </ToolBar>
                                    </children>
                                </GridPane>
                                <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1">
                                    <children>
                                        <PropertySaveTableView fx:id="workList" editable="true" layoutX="49.0" layoutY="152.0" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                            <columns>
                                                <TableColumn fx:id="selectedColumn" editable="true" prefWidth="40.0" />
                                                <TableColumn fx:id="workIdColumn" editable="true" prefWidth="150.0" text="%key.IndirectWorkNumber" />
                                                <TableColumn fx:id="workNameColumn" editable="true" prefWidth="300.0" text="%key.IndirectWorkName" />
                                            </columns>
                                        </PropertySaveTableView>
                                    </children>
                                </AnchorPane>
                                <AnchorPane GridPane.rowIndex="2">
                                    <children>
                                        <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                            <columnConstraints>
                                                <ColumnConstraints hgrow="SOMETIMES" />
                                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" />
                                            </columnConstraints>
                                            <rowConstraints>
                                                <RowConstraints vgrow="SOMETIMES" />
                                            </rowConstraints>
                                            <children>
                                                <ToolBar nodeOrientation="LEFT_TO_RIGHT" prefHeight="40.0">
                                                    <items>
                                                        <Button fx:id="addWorkButton" mnemonicParsing="false" onAction="#onAddWork" styleClass="NarrowWidthButton" text="%key.Add" />
                                                        <Button fx:id="deleteWorkButton" mnemonicParsing="false" onAction="#onDeleteWork" styleClass="NarrowWidthButton" text="%key.Delete" />
                                                    </items>
                                                </ToolBar>
                                                <ToolBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="100" GridPane.columnIndex="1">
                                                    <items>
                                                        <Button fx:id="registWorkButton" mnemonicParsing="false" onAction="#onRegistWork" styleClass="NarrowWidthButton" text="%key.Regist" />
                                                    </items>
                                                </ToolBar>
                                            </children>
                                        </GridPane>
                                    </children>
                                </AnchorPane>
                            </children>
                        </GridPane>
                    </items>
                </SplitPane>
                <Pane fx:id="Progress" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity">
                    <children>
                        <ProgressIndicator maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" />
                    </children>
                </Pane>
            </children>
        </StackPane>
    </children>
</AnchorPane>
