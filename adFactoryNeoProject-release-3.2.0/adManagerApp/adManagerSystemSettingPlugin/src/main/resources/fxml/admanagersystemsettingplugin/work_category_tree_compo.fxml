<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="adtekfuji.admanagerapp.systemsettingplugin.component.WorkCategoryTreeCompoFxController">
    <children>
          <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <children>
                  <TreeView fx:id="categoryTree" />
                  <HBox GridPane.rowIndex="1">
                      <children>
                          <Label fx:id="itemElementName" prefHeight="24.0" prefWidth="60.0" textOverrun="CLIP">
                              <HBox.margin>
                                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                              </HBox.margin>
                          </Label>
                          <TextField fx:id="selectedItemName" editable="false" prefHeight="24.0">
                              <HBox.margin>
                                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                              </HBox.margin>
                          </TextField>
                      </children>
                      <GridPane.margin>
                          <Insets />
                      </GridPane.margin>
                  </HBox>
              </children>
              <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
              </columnConstraints>
              <rowConstraints>
                  <RowConstraints vgrow="SOMETIMES" />
                  <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="50.0" vgrow="SOMETIMES" />
              </rowConstraints>
          </GridPane>
    </children>
</AnchorPane>
