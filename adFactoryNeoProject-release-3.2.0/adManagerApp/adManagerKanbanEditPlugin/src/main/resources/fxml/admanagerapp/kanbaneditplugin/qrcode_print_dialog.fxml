<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import jp.adtekfuji.javafxcommon.controls.*?>

<AnchorPane fx:id="anchorPane" prefWidth="320.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="adtekfuji.admanagerapp.kanbaneditplugin.dialog.QRCodePrintDialog">
    <StackPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <VBox>
            <AnchorPane prefHeight="100" >
                <children>
                    <!-- コンボボックス -->
                    <HBox alignment="CENTER" spacing="16.0" AnchorPane.bottomAnchor="25.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                        <children>
                            <Label text="%key.printNum" />
                            <ComboBox fx:id="printNumComboBox" prefWidth="100.0" editable="true" />
                        </children>
                    </HBox>
                </children>
            </AnchorPane>
            <HBox alignment="CENTER_RIGHT" spacing="8.0">
                <children>
                    <Button mnemonicParsing="false" onAction="#onPrintButton" styleClass="ContentButton" text="%key.Printed" />
                    <Button mnemonicParsing="false" onAction="#onCancelButton" styleClass="ContentButton" text="%key.Cancel" />
                </children>
            </HBox>
        </VBox>
        <Pane fx:id="Progress" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" >
            <children>
                <ProgressIndicator/>
            </children>
        </Pane>
    </StackPane>
    <padding>
        <Insets bottom="-6.0" left="12.0" right="12.0" top="12.0" />
    </padding>
</AnchorPane>
