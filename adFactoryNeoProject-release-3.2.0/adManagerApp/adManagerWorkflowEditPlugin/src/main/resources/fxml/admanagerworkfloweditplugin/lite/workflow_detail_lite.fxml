<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import jp.adtekfuji.javafxcommon.controls.*?>

<GridPane fx:id="content" minHeight="0.0" minWidth="0.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="adtekfuji.admanagerapp.workfloweditplugin.component.lite.WorkflowDetailLite">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
        <RowConstraints vgrow="SOMETIMES" />
        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="60.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
          <VBox fillWidth="false">
              <children>
                  <VBox fx:id="defaultFieldPane" fillWidth="false" VBox.vgrow="SOMETIMES">
                      <VBox.margin>
                          <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                      </VBox.margin>
                  </VBox>
                  <VBox fx:id="ledgerPathFieldPane" fillWidth="false" VBox.vgrow="SOMETIMES">
                      <VBox.margin>
                          <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                      </VBox.margin>
                  </VBox>
                  <VBox fx:id="workflowViewPane" alignment="TOP_LEFT" fillWidth="false" spacing="5" VBox.vgrow="SOMETIMES">
                      <children>
                          <Label styleClass="ContentTitleLabel" text="%key.DialogWorkSettiong" />
                      </children>
                      <VBox.margin>
                          <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                      </VBox.margin>
                  </VBox>
              </children>
          </VBox>
      <GridPane style="-fx-border-width: 1; -fx-border-color: black;" GridPane.rowIndex="1">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
            <RowConstraints vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <ScrollPane fitToHeight="true" fitToWidth="true" GridPane.rowIndex="1">
               <content>
                  <VBox fx:id="workListPane" >
                     <padding>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                     </padding>
                  </VBox>
               </content>
            </ScrollPane>
            <HBox alignment="CENTER_LEFT" spacing="10.0">
               <children>
                  <Label styleClass="ContentTitleLabel" text="%key.productionWay" />
                  <ComboBox fx:id="prodWayCombo" styleClass="ContentComboBox" />
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
               </padding>
            </HBox>
         </children>
      </GridPane>
        <AnchorPane GridPane.rowIndex="2">
            <children>
                  <HBox alignment="CENTER_RIGHT" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                      <children>
                          <ToolBar nodeOrientation="RIGHT_TO_LEFT" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                              <items>
                                  <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#onCancel" styleClass="NarrowWidthButton" text="%key.Cancel" />
                                  <Button fx:id="registButton" mnemonicParsing="false" onAction="#onRegist" styleClass="NarrowWidthButton" text="%key.Regist" />
                              </items>
                          </ToolBar>
                      </children>
                  </HBox>
            </children>
        </AnchorPane>
    </children>
</GridPane>
