<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="adtekfuji.admanagerapp.workfloweditplugin.component.WorkflowDetailCompoFxController">
    <children>
        <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="50.0" vgrow="SOMETIMES" />
                <RowConstraints vgrow="SOMETIMES" />
                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="50.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <ScrollPane GridPane.rowIndex="1" fitToWidth="true" fitToHeight="false">
                    <content>
                        <VBox>
                            <children>
                                <VBox fx:id="defaultFieldPane" VBox.vgrow="SOMETIMES">
                                    <VBox.margin>
                                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                                    </VBox.margin>
                                </VBox>
                                <VBox fx:id="ledgerPathFieldPane" VBox.vgrow="SOMETIMES">
                                    <VBox.margin>
                                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                                    </VBox.margin>
                                </VBox>
                                <VBox fx:id="workflowViewPane" alignment="TOP_LEFT" fillWidth="true" VBox.vgrow="SOMETIMES" spacing="5">
                                    <children>
                                        <Label styleClass="ContentTitleLabel" text="%key.workflow" />
                                        <HBox alignment="BOTTOM_LEFT" fillHeight="false">
                                            <children>
                                                <Button fx:id="editOrderProcessesButton" onAction="#onWorkflowEdit" styleClass="ContentButton" text="%key.EditOrderProcesses">
                                                    <VBox.margin>
                                                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                                                    </VBox.margin>
                                                </Button>
                                                <Pane HBox.hgrow="SOMETIMES"/>
                                                <HBox alignment="CENTER_RIGHT" spacing="10">
                                                    <children>
                                                        <ToggleButton fx:id="visibleOrganizationToggleButton" text="%key.OrganizationName" />
                                                        <ToggleButton fx:id="visibleEquipmentToggleButton" text="%key.EquipmentName" />
                                                        <ToggleButton fx:id="visibleScaleToggleButton" selected="true" prefHeight="20.0" prefWidth="20.0" styleClass="ScalingButton" stylesheets="@../../styles/workflowStyles.css" />
                                                    </children>
                                                </HBox>
                                            </children>
                                            <padding>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                            </padding>
                                        </HBox>    
                                        <StackPane>
                                            <children>
                                                <ScrollPane fx:id="scrollPane" fitToHeight="true" hbarPolicy="ALWAYS">
                                                    <content>
                                                    </content>
                                                </ScrollPane>
                                                <GridPane fx:id="zoomPane" maxHeight="100.0" maxWidth="30.0" StackPane.alignment="TOP_RIGHT">
                                                    <columnConstraints>
                                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="0.0" prefWidth="15.0" />
                                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="0.0" prefWidth="15.0" />
                                                    </columnConstraints>
                                                    <rowConstraints>
                                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                                    </rowConstraints>
                                                    <children>
                                                        <Slider fx:id="zoomSlider" majorTickUnit="0.1" max="10.0" maxHeight="100.0" min="1.0" orientation="VERTICAL" showTickLabels="false" showTickMarks="false" GridPane.rowSpan="2" />
                                                        <Label alignment="TOP_CENTER" text="+" GridPane.columnIndex="1" GridPane.valignment="TOP" />
                                                        <Label alignment="BOTTOM_CENTER" text="-" GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.valignment="BOTTOM" />
                                                    </children>
                                                    <StackPane.margin>
                                                        <Insets right="5.0" />
                                                    </StackPane.margin>
                                                </GridPane>
                                            </children>
                                        </StackPane>
                                    </children>
                                    <VBox.margin>
                                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                                    </VBox.margin>
                                </VBox>
                                <VBox fx:id="separateworkFieldPane">
                                    <VBox.margin>
                                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                                    </VBox.margin>
                                </VBox>
                                <VBox fx:id="propertyFieldPane">
                                    <VBox.margin>
                                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                                    </VBox.margin>
                                </VBox>
                            </children>
                        </VBox>
                    </content>
                    <GridPane.margin>
                        <Insets bottom="8.0" />
                    </GridPane.margin>
                </ScrollPane>
                <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="2">
                    <children>
                        <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" />
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints vgrow="SOMETIMES" />
                            </rowConstraints>
                            <children>
                                <ToolBar nodeOrientation="LEFT_TO_RIGHT" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                    <items>
                                        <Button fx:id="requestButton" mnemonicParsing="false" onAction="#onRequest" styleClass="NarrowWidthButton" text="%key.Approval" />
                                        <Button fx:id="requestCancelButton" mnemonicParsing="false" onAction="#onRequestCancel" styleClass="NarrowWidthButton" text="%key.ApprovalCancel" />
                                    </items>
                                </ToolBar>
                                <HBox alignment="BASELINE_RIGHT" GridPane.columnIndex="1">
                                    <children>
                                        <ToolBar nodeOrientation="RIGHT_TO_LEFT" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                            <items>
                                                <Button fx:id="applyButton" mnemonicParsing="false" onAction="#onApply" styleClass="NarrowWidthButton" text="%key.Apply" />
                                                <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#onCancel" styleClass="NarrowWidthButton" text="%key.Cancel" />
                                                <Button fx:id="registButton" mnemonicParsing="false" onAction="#onRegist" styleClass="NarrowWidthButton" text="%key.Regist" />
                                            </items>
                                        </ToolBar>
                                    </children>
                                </HBox>
                            </children>
                        </GridPane>
                    </children>
                </AnchorPane>
                <HBox alignment="CENTER_LEFT">
                    <children>
                        <Label fx:id="parentNameLabel" styleClass="ContentTitleLabel" />
                    </children>
                </HBox>
                <HBox alignment="CENTER_RIGHT">
                    <children>
                        <Label fx:id="requestingLabel" styleClass="ContentTitleLabel" text="%key.StateRequesting">
                            <HBox.margin>
                                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"  />
                            </HBox.margin>
                        </Label>
                        <Label fx:id="requestorNameLabel" styleClass="ContentTitleLabel">
                            <HBox.margin>
                                <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"  />
                            </HBox.margin>
                        </Label>
                    </children>
                </HBox>
            </children>
        </GridPane>
    </children>
</AnchorPane>
