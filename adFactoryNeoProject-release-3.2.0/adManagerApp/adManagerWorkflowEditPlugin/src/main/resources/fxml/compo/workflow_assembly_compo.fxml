<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="adtekfuji.admanagerapp.workfloweditplugin.component.WorkflowAssemblyCompoFxController">
    <children>
        <StackPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <SplitPane dividerPositions="0.35" styleClass="ContentSplitPane">
                    <items>
                        <GridPane SplitPane.resizableWithParent="false">
                            <children>
                                <TreeView fx:id="workTree" onKeyPressed="#onKeyPressed">
                                    <GridPane.margin>
                                        <Insets bottom="8.0" />
                                    </GridPane.margin>
                                </TreeView>
                                <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1">
                                    <children>
                                        <ToolBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                            <items>
                                                <Button fx:id="addSeriesButton" mnemonicParsing="false" onAction="#onAddSerial" styleClass="NarrowWidthButton" text="%key.Series" />
                                                <Button fx:id="addParallelButton" mnemonicParsing="false" onAction="#onAddParallel" styleClass="NarrowWidthButton" text="%key.Parallel" />
                                            </items>
                                        </ToolBar>
                                    </children>
                                </AnchorPane>
                            </children>
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints vgrow="SOMETIMES" />
                                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="50.0" vgrow="SOMETIMES" />
                            </rowConstraints>
                        </GridPane>
                        <GridPane>
                            <children>
                                <BorderPane prefHeight="200.0" prefWidth="200.0">
                                    <right>
                                        <HBox alignment="BOTTOM_RIGHT" prefHeight="100.0" prefWidth="400.0" BorderPane.alignment="CENTER" spacing="4.0" >
                                            <children>
                                                <Button fx:id="editWorkButton" mnemonicParsing="false" onAction="#onWorkEdit" styleClass="NarrowWidthButton" text="%key.Edit" />
                                                <Button fx:id="deleteWorkButton" mnemonicParsing="false" onAction="#onWorkDelete" styleClass="NarrowWidthButton" text="%key.Delete" />
                                                <Button fx:id="allCheckButton" mnemonicParsing="false" onAction="#onAllCheck" styleClass="NarrowWidthButton" text="%key.CheckAll">
                                                    <HBox.margin>
                                                        <Insets left="8.0" />
                                                    </HBox.margin>
                                                </Button>
                                                <Button fx:id="allUncheckButton" mnemonicParsing="false" onAction="#onAllUncheck" prefWidth="150" styleClass="NarrowWidthButton" text="%key.ReleaseCheckAll" />
                                            </children>
                                            <BorderPane.margin>
                                                <Insets bottom="4.0" />
                                            </BorderPane.margin>
                                        </HBox>
                                    </right>
                                    <left>
                                        <Label fx:id="workflowName" maxWidth="400.0" styleClass="ContentTitleLabel" text="工程順名" BorderPane.alignment="CENTER">
                                            <BorderPane.margin>
                                                <Insets left="4.0" />
                                            </BorderPane.margin>
                                        </Label>
                                    </left>
                                </BorderPane>
                                <VBox fx:id="workflowVBox" GridPane.rowIndex="1">
                                    <children>
                                        <HBox alignment="CENTER_RIGHT" spacing="10">
                                            <children>
                                                <ToggleButton fx:id="visibleOrganizationToggleButton" text="%key.OrganizationName" />
                                                <ToggleButton fx:id="visibleEquipmentToggleButton" text="%key.EquipmentName" />
                                                <ToggleButton fx:id="visibleScaleToggleButton" selected="true" prefHeight="20.0" prefWidth="20.0" styleClass="ScalingButton" stylesheets="@../../styles/workflowStyles.css" />
                                            </children>
                                            <padding>
                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                            </padding>
                                        </HBox>
                                        <StackPane>
                                            <children>
                                                <ScrollPane fx:id="workflowScrollPane">
                                                    <content>
                                                    </content>
                                                </ScrollPane>
                                                <GridPane fx:id="zoomPane" maxHeight="100.0" maxWidth="30.0" StackPane.alignment="TOP_RIGHT">
                                                    <columnConstraints>
                                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="0.0" prefWidth="15.0" />
                                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="0.0" prefWidth="15.0" />
                                                    </columnConstraints>
                                                    <rowConstraints>
                                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                                    </rowConstraints>
                                                    <children>
                                                        <Slider fx:id="zoomSlider" majorTickUnit="0.1" max="10.0" maxHeight="100.0" min="1.0" orientation="VERTICAL" showTickLabels="false" showTickMarks="false" GridPane.rowSpan="2" />
                                                        <Label alignment="TOP_CENTER" text="+" GridPane.columnIndex="1" GridPane.valignment="TOP" />
                                                        <Label alignment="BOTTOM_CENTER" text="-" GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.valignment="BOTTOM" />
                                                    </children>
                                                    <StackPane.margin>
                                                        <Insets right="15.0" />
                                                    </StackPane.margin>
                                                </GridPane>
                                            </children>
                                        </StackPane>
                                    </children>
                                    <GridPane.margin>
                                        <Insets bottom="8.0" />
                                    </GridPane.margin>
                                </VBox>
                                <AnchorPane prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="2">
                                    <children>
                                        <ToolBar nodeOrientation="RIGHT_TO_LEFT" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                            <items>
                                                <Button fx:id="applyButton" mnemonicParsing="false" onAction="#onApply" styleClass="NarrowWidthButton" text="%key.Apply" />
                                                <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#onCancel" styleClass="NarrowWidthButton" text="%key.Cancel" />
                                                <Button fx:id="createWorkflowButton" mnemonicParsing="false" onAction="#onWorkflowSave" styleClass="NarrowWidthButton" text="%key.Regist" />
                                            </items>
                                        </ToolBar>
                                    </children>
                                </AnchorPane>
                            </children>
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="50.0" vgrow="SOMETIMES" />
                                <RowConstraints vgrow="SOMETIMES" />
                                <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="50.0" vgrow="SOMETIMES" />
                            </rowConstraints>
                        </GridPane>
                    </items>
                </SplitPane>
                <Pane fx:id="assemblyProgress" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" StackPane.alignment="CENTER">
                    <children>
                        <ProgressIndicator maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" />
                    </children>
                </Pane>
            </children>
        </StackPane>
    </children>
</AnchorPane>
