<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane fx:id="anchorPane" prefHeight="270.0" prefWidth="469.0" xmlns="http://javafx.com/javafx/17"
           xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="adtekfuji.admanagerapp.workfloweditplugin.component.WorkflowDataCheckDialog">
    <VBox spacing="8.0">
        <HBox alignment="CENTER_LEFT" minWidth="-Infinity" prefHeight="50.0" prefWidth="469.0" spacing="8.0">
            <Label text = "%key.DataCheckErrorComment"/>
        </HBox>
        <!-- リスト -->
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="442.0" GridPane.rowIndex="1">
            <TableView fx:id="dataCheckListTableView" layoutX="18.0" layoutY="1.0" prefHeight="169.0"
                       prefWidth="442.0" styleClass="ContentList"
                       AnchorPane.bottomAnchor="6.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="13.0"
                       AnchorPane.topAnchor="1.0">
                <columns>
                    <!-- レベル -->
                    <TableColumn fx:id="levelColumn" prefWidth="65.0" text="%key.DataCheckErrorLevel"/>
                    <!--名称-->
                    <TableColumn fx:id="errorTypeColumn" prefWidth="181.0" text="%key.DataCheckErrorType"/>
                    <!-- 内容 -->
                    <TableColumn fx:id="messageColumn" prefWidth="198.0" text="%key.DataCheckErrorMessage"/>
                </columns>
            </TableView>
        </AnchorPane>
        <AnchorPane prefHeight="20.0">
            <HBox alignment="CENTER" spacing="8.0" AnchorPane.rightAnchor="0.0" prefWidth="469.0">
                <!-- OK -->
                <Button mnemonicParsing="false" onAction="#onOKButton" styleClass="ContentButton"
                        text="%key.Ok"/>
            </HBox>
        </AnchorPane>
    </VBox>
</StackPane>
