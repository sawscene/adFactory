<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" 
            fx:controller="adtekfuji.admanagerapp.warehouseplugin.dialog.PrintWithdrawalOrderDialog">
    <children>
        <StackPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <GridPane>
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="0.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints/>
                        <RowConstraints vgrow="ALWAYS"/>
                        <RowConstraints/>
                    </rowConstraints>
                    <children>
                        <Label styleClass="ContentTitleLabel" text="%printWithdrawalOrder" />
                        <HBox spacing="10.0" prefHeight="150.0" alignment="CENTER" GridPane.rowIndex="1">
                            <children>
                                <Label styleClass="ContentTitleLabel" text="%key.printNum" />
                                <ComboBox fx:id="copiesComboBox" styleClass="ContentComboBox" prefWidth="200.0" />
                            </children>
                        </HBox>
                        <HBox alignment="CENTER_RIGHT" spacing="10.0" GridPane.rowIndex="2">
                            <children>
                                <Button fx:id="printButton" mnemonicParsing="false" onAction="#onPrint" styleClass="ContentButton" text="%key.Printed" />
                                <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#onCancel" styleClass="ContentButton" text="%key.Cancel" />
                            </children>
                            <padding>
                                <Insets bottom="0.0" left="10.0" right="10.0" top="10.0" />
                            </padding>
                        </HBox>
                    </children>
                    <padding>
                        <Insets bottom="0.0" left="8.0" right="8.0" top="8.0" />
                    </padding>
                </GridPane>
                <!-- 処理中表示 -->
                <Pane fx:id="progressPane" visible="false" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" StackPane.alignment="CENTER">
                    <children>
                        <ProgressIndicator />
                    </children>
                </Pane>
            </children>
        </StackPane>
    </children>
</AnchorPane>
