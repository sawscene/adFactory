<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints vgrow="SOMETIMES" />
            <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="50.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <TreeView fx:id="hierarchyTree" layoutX="100.0" layoutY="133.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
            <AnchorPane id="AnchorPane" GridPane.rowIndex="1">
               <children>
                  <ToolBar fx:id="hierarchyBtnArea" layoutX="345.0" layoutY="143.0" nodeOrientation="RIGHT_TO_LEFT" styleClass="ContentToolbar" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <items>
                        <Button fx:id="authButton" mnemonicParsing="false" onAction="#onAuthButton" styleClass="SimpleButton" prefHeight="40.0" text="%key.EditedAuth" >
                        <tooltip>
                            <Tooltip text="%key.AuthUnitHierarchy" />
                        </tooltip></Button>
                        <Button fx:id="createButton" mnemonicParsing="false" onAction="#onCreateButton" styleClass="SimpleButton">
                           <graphic>
                              <VBox alignment="CENTER">
                                 <children>
                                    <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                                       <image>
                                          <Image url="@../../../image/createFile.png" />
                                       </image>
                                    </ImageView>
                                 </children>
                              </VBox>
                           </graphic>
                           <tooltip>
                              <Tooltip text="%key.CreateUnitHierarchy" />
                           </tooltip></Button>
                        <Button fx:id="editButton" mnemonicParsing="false" onAction="#onEditButton" styleClass="SimpleButton">
                           <graphic>
                              <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../../image/edit.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                           <tooltip>
                              <Tooltip text="%key.EditUnitHierarchy" />
                           </tooltip></Button>
                        <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#onDeleteButton" styleClass="SimpleButton">
                           <graphic>
                              <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../../image/delete.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                           <tooltip>
                              <Tooltip text="%key.DeleteUnitHierarchy" />
                           </tooltip></Button>
                     </items>
                  </ToolBar>
               </children>
            </AnchorPane>
         </children>
      </GridPane>
   </children>
</AnchorPane>
