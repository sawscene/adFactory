<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import jp.adtekfuji.javafxcommon.controls.*?>

<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
      <GridPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints vgrow="SOMETIMES" />
            <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" prefHeight="50.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
              <PropertySaveTableView fx:id="unittemplateTable" styleClass="ContentTable" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                  <columns>
                      <TableColumn fx:id="unittemplateColumn" prefWidth="75.0" styleClass="ContentTable" text="%key.unittemplateName" />
                      <TableColumn fx:id="updateByColumn" prefWidth="75.0" styleClass="ContentTable" text="%key.UpdatedBy" />
                      <TableColumn fx:id="updateDateColumn" prefWidth="75.0" styleClass="ContentTable" text="%key.UpdatedDate" />
                  </columns>
                  <columnResizePolicy>
                      <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                  </columnResizePolicy>
              </PropertySaveTableView>
            <AnchorPane GridPane.rowIndex="1">
               <children>
                  <ToolBar nodeOrientation="RIGHT_TO_LEFT" styleClass="ContentToolbar" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" GridPane.rowIndex="1">
                     <items>
                        <Button fx:id="createButton" mnemonicParsing="false" onAction="#onCreateButton" styleClass="SimpleButton">
                           <graphic>
                              <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../../image/createFile.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                           <tooltip>
                              <Tooltip text="%key.CreateUnitTemplate" />
                           </tooltip></Button>
                        <Button fx:id="editButton" mnemonicParsing="false" onAction="#onEditButton" styleClass="SimpleButton">
                           <graphic>
                              <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../../image/edit.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                           <tooltip>
                              <Tooltip text="%key.EditUnitTemplate" />
                           </tooltip></Button>
                        <Button fx:id="copyButton" mnemonicParsing="false" onAction="#onCopyButton" styleClass="SimpleButton">
                           <graphic>
                              <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../../image/copy.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                           <tooltip>
                              <Tooltip text="%key.CopyUnitTemplate" />
                           </tooltip></Button>
                        <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#onDeleteButton" styleClass="SimpleButton">
                           <graphic>
                              <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../../image/delete.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                           <tooltip>
                              <Tooltip text="%key.DeleteUnitTemplate" />
                           </tooltip></Button>
                     </items>
                  </ToolBar>
               </children>
            </AnchorPane>
         </children>
      </GridPane>
    </children>
</AnchorPane>
