<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jp.adtekfuji.forfujiapp.dialog.WorkKanbanSettingDialog">
    <children>
        <StackPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <ScrollPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <content>
                        <VBox>
                            <children>
                                <GridPane hgap="8.0" vgap="8.0">
                                    <columnConstraints>
                                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" />
                                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="-Infinity" />
                                        <ColumnConstraints hgrow="SOMETIMES" />
                                    </columnConstraints>
                                    <rowConstraints>
                                        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
                                        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
                              <RowConstraints vgrow="SOMETIMES" />
                                        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
                                        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
                                        <RowConstraints maxHeight="-Infinity" minHeight="-Infinity" vgrow="SOMETIMES" />
                                    </rowConstraints>
                                    <children>
                                        <Label text="%key.TactTimeTitle" />
                                        <Label text="%key.OffsetTimeTitle" GridPane.rowIndex="1" />
                                        <Label text="%key.Skip" GridPane.rowIndex="3" />
                                        <VBox GridPane.rowIndex="4">
                                            <children>
                                                <Label text="%key.Equipment" GridPane.rowIndex="3" />
                                            </children>
                                        </VBox>
                                        <VBox GridPane.rowIndex="5">
                                            <children>
                                                <Label text="%key.Organization" GridPane.rowIndex="4" />
                                            </children>
                                        </VBox>
                                        <CheckBox fx:id="skipCheck" mnemonicParsing="false" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                                        <TextField fx:id="TactTimeTextField" GridPane.columnIndex="1" />
                                        <TextField fx:id="OffsetTimeTextField" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                                        <ListView fx:id="equipmentsList" prefHeight="100.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                                        <ListView fx:id="organizationList" prefHeight="100.0" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                                        <VBox alignment="BOTTOM_LEFT" spacing="8.0" GridPane.columnIndex="2" GridPane.rowIndex="4">
                                            <children>
                                                <Button mnemonicParsing="false" onAction="#OnEditEquipment" text="%key.EditEquipmentTitle" />
                                            </children>
                                        </VBox>
                                        <VBox alignment="BOTTOM_LEFT" spacing="8.0" GridPane.columnIndex="2" GridPane.rowIndex="5">
                                            <children>
                                                <Button mnemonicParsing="false" onAction="#OnEditOrganization" text="%key.EditOrgnizationTitle" />
                                            </children>
                                        </VBox>
                              <VBox spacing="8.0" GridPane.rowIndex="2">
                                 <children>
                                    <RadioButton fx:id="radioOffset" mnemonicParsing="false" selected="true" text="%key.OffsetTime">
                                       <toggleGroup>
                                          <ToggleGroup fx:id="offsetSetting" />
                                       </toggleGroup>
                                    </RadioButton>
                                    <RadioButton fx:id="radioStartTimeOffset" mnemonicParsing="false" text="%key.StandardStartTime" toggleGroup="$offsetSetting" />
                                 </children>
                                 <padding>
                                    <Insets left="8.0" />
                                 </padding>
                              </VBox>
                                    </children>
                                </GridPane>
                                <VBox fx:id="propertyPane" />
                            </children>
                        </VBox>
                    </content>
                    <padding>
                        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
                    </padding>
                </ScrollPane>
                <Pane fx:id="progressPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity">
                    <children>
                        <ProgressIndicator maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" />
                    </children>
                </Pane>
            </children>
        </StackPane>
    </children>
</AnchorPane>
